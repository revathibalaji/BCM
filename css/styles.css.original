@font-face {
    font-family: MyriadProBold;
    src: url(../fonts/MyriadPro-Bold.otf);
    font-weight: 800;
}

@font-face {
    font-family: MyriadProRegular;
    src: url(../fonts/MyriadPro-Regular.otf);
    font-weight: normal;
}

html, body {
	font-size: 100%;
	font-family: Helvetica, Arial, sans-serif;;
	line-height: 1;
}
body {
	background:#fff;
	color: #6f6f6f;
}
h1, h2, h3, h4, h5, h6 {
	margin: 0;
	color: #51514e;
}
.clear {
	clear: both;
	height: 0;
}
.clearfix:after {
   content: ".";
   visibility: hidden;
   display: block;
   height: 0;
   clear: both;
}
a, a img, a:focus {
	outline: none;
}
a {
	text-decoration: none;
	color:#51514e;
}
a:hover {
	color: #3c7fed;
}
p {
	margin: 0;
	font-size: 100%;
	line-height: 1.5;
}
h1 {
	font-size: 3.2em;
	text-transform: uppercase;
	text-shadow: 0px -1px 0px rgba(50, 50, 50, 0.5);
	padding: 40px 0;
}
h2 {
	font-size: 2.8em;
	border-bottom: 1px solid #fff;
	padding: 20px 0 5px 8px;
	margin-bottom: 20px;
	text-shadow: 0px -1px 0px rgba(50, 50, 50, 0.5);
}
h3 {
	font-size: 1.5em;
	padding-bottom: 15px;
}
h4 {
	color: #032a2f;
	font-size: 1em;
	padding-bottom: 5px;
}
ul {
	margin: 0;
	padding: 0;
	font-size: 100%;
}

#header-wrapper {
	background: #91c2e0;
	border-bottom: 1px solid #8be0ff;
	height: 95px;
	min-width: 960px;
}
#header-inner {
	
}
#logo {
	margin-top: 25px;
	margin-left: 20px;
	display: block;
	float: left;
}

#user-box {
	float: right;
	margin-right: 20px;
	margin-top: 25px;
}
#user-links {
	float: left;
	padding-top: 2px;
}
#user-links a {
	color: #fff;
	display: block;
	text-align: right;
	padding: 2px 0;
	text-shadow: 0 1px 1px rgba(0,0,0,0.2);
}
#user-links a:hover {
	color: #494a4a;
}
#user-box img {
	display: block;
	float: right;
	margin-left: 10px;
	width: 41px;
	height: 41px;
}
#menu-wrapper {
	min-width: 960px;
}
#menu-top {
	background: url(../img/menu-top-bg.jpg) repeat-x #262727;
	height: 40px;
}
#menu-top ul#menu-items {
	overflow: hidden;
}
#menu-top ul#menu-items li {
	float: left;
	padding: 0 20px;
}
#menu-top ul#menu-items li a {
	display: block;
	text-align: center;
	color: #fff;
	line-height: 40px;
	text-shadow: 0 1px 1px rgba(0,0,0,0.2);
}
#menu-top ul#menu-items li a:hover {
	color: #48bee9;
}
#menu-top ul#menu-items li a.active {
	background: url(../img/menu-active.jpg) no-repeat center bottom;
}
#menu-sub {
	background: #f2f2f2;
	height: 28px;
}
#menu-sub ul {
	padding-left: 6px;
}
#menu-sub ul li {
	float: left;
	padding: 0 14px;
}
#menu-sub ul li a {
	display: block;
	text-align: center;
	line-height: 28px;
	font-size: 0.7em;
}
#menu-sub ul li a:hover {
	
}
#controls {
	float: right;
}
#controls li{
	cursor: pointer;
}
#help-control {
	float: left;
	border-left: 1px solid rgba(66,66,66,0.6);
	box-shadow: -1px 0 0 rgba(80,80,80,0.6);
}
#help-control-icon {
	background: url(../img/help-icon.jpg) no-repeat center center;
	width: 46px;
	height: 40px;
}
#search-control {
	float: left;
	border-left: 1px solid rgba(66,66,66,0.6);
	box-shadow: -1px 0 0 rgba(80,80,80,0.6);
}
#search-control-icon {
	background: url(../img/search-icon.jpg) no-repeat center center;
	width: 46px;
	height: 40px;
	display: inline-block;
	float: right;
}
#search-field{
	width: 150px;
	height: 40px;
	line-height: 40px;
	display: inline-block;
	float: left;
}
#search-field input{
	width: 146px;
	background: rgba(0,0,0,0.4);
	border: none;
	height: 26px;
	margin: 2px 0px;
	color: #36b0e3;
	font-size: 12px;
	padding: 0px 2px;
}
#add-control {
	float: left;
	background: url(../img/add-control-bg.jpg) repeat-x;
	border-left: 1px solid rgba(66,66,66,0.6);
	box-shadow: -1px 0 0 rgba(80,80,80,0.6);
}
#add-control-icon {
	background: url(../img/add-icon.jpg) no-repeat center center;
	width: 46px;
	height: 40px;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

#add-control-items{
	position: absolute;
	margin-top: 40px;
	width: 150px;
	right: 0px;
	background: #ffffff;
	-webkit-box-shadow: -2px 3px 5px rgba(50, 50, 50, 0.4);
	-moz-box-shadow:    -2px 3px 5px rgba(50, 50, 50, 0.4);
	box-shadow:         -2px 3px 5px rgba(50, 50, 50, 0.4);
}

#add-control-items ul{
	list-style: none;
}

#add-control-items ul li{
	height: 35px;
	line-height: 35px;
	padding: 0 10px;
	border-bottom: 1px solid #f2f2f2;
}

#content-wrapper {
	min-width: 920px;
	padding: 30px 20px;
}

/* --- Dashboard Area --- */
#content-wrapper #widgets-area-wrap{
	position: relative;
}
#content-wrapper #main-area{
	float: left;
	padding: 15px;
	padding-bottom: 0;
	border: 1px solid #c7c7c7;
	border-radius: 5px; 
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px;
	/*margin-right: 370px ;*/
}
#content-wrapper #side-area{
	position: absolute;
	top: 0;
	right: 0;
	width: 360px;
}
#content-wrapper .widget{
	float: left;
	width: 100%;
	margin-bottom: 15px;
}
#content-wrapper .widget .widget-header{
	display: block;
	padding: 10px;
	background-color: #00a7e3;
	border-radius: 5px 5px 0 0; 
	-moz-border-radius: 5px 5px 0 0; 
	-webkit-border-radius: 5px 5px 0 0;
	height: 15px;
	border: 1px solid #34a1c8;
	color: #ffffff;
	font-size: 16px;
	font-family: "MyriadProBold";
	text-shadow: 0 1px 1px rgba(0,0,0,0.75);
}
#content-wrapper .widget .widget-content{
	display: block;
	border: 1px solid #34a1c8;
	border-top: 0;
	font-family: "MyriadProRegular";
	font-size: 14px;
	color: #51514e;
}
#content-wrapper .widget .widget-content p{
	padding: 10px;
}
#content-wrapper .widget .widget-content ul{
	list-style-type: none;
	
}

#content-wrapper .widget .widget-content ul li{
	font-family: "MyriadProRegular";
	font-size: 14px;
	color: #51514e;
	padding: 15px;
}
#content-wrapper .widget .widget-content ul li:nth-child(2n+2){
	background-color: #dcf6ff;
}
#content-wrapper .widget .widget-content ul li a.more{
	float: right;
	color: #91c2e0;
}
#content-wrapper .widget .widget-content ul li a.more:hover{
	text-decoration: underline;
}
#content-wrapper #side-area .widget .widget-content{
	border: 1px solid #c7c7c7;
}
#content-wrapper .widget.one-half{
	width: 49% !important;
	float: left;
}
#content-wrapper .widget.second-half{
	width: 49% !important;
	float: right;
}

.main-table {
	width: 100%;
	border-spacing: 0;
	border: 1px solid #c7c7c7;
	border-radius: 4px;
	border-collapse: separate;
	font-size: 0.875em;
	box-shadow: 0 1px 2px #CCCCCC;
	clear: both;
	margin: 15px 0;
}
.main-table tr th {
	background: rgb(239,239,239);
	background: -moz-linear-gradient(top,  rgba(252,252,252,1) 0%, rgba(239,239,239,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(252,252,252,1)), color-stop(100%,rgba(239,239,239,1)));
	background: -webkit-linear-gradient(top,  rgba(252,252,252,1) 0%,rgba(239,239,239,1) 100%);
	background: -o-linear-gradient(top,  rgba(252,252,252,1) 0%,rgba(239,239,239,1) 100%);
	background: -ms-linear-gradient(top,  rgba(252,252,252,1) 0%,rgba(239,239,239,1) 100%);
	background: linear-gradient(to bottom,  rgba(252,252,252,1) 0%,rgba(239,239,239,1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fcfcfc', endColorstr='#efefef',GradientType=0 );
	border-top: medium none;
	padding: 13px 10px;
	text-align: left;
}
.main-table tr th:first-child,
.main-table tr th:last-child {
	border-left: medium none;
}
.main-table tr th:first-child {
	border-radius: 4px 0 0 0;
}
.main-table tr th:last-child {
	border-radius: 0 4px 0 0;
}
.main-table tr th a {
	color: #51514E;
}
.main-table tr th a:hover,
.main-table tr th a.asc,
.main-table tr th a.desc {
	color: #30aade;
}
.main-table tbody tr td {
    border-top: 1px solid #e7e7e7;
    border-right: 1px solid #e7e7e7;
    padding: 10px;
    text-align: left;
    vertical-align: top;
    font-size: 0.875em;
}
.main-table tr.even {
	background: #fafafa;
}
.main-table tbody tr:hover {
	background: #e1f7ff;
	transition: all 0.1s ease-in-out 0s;
}
.main-table tr:first-child td {
    border-top-color: #c7c7c7;
}
.main-table tr:last-child td:first-child {
    border-radius: 0 0 0 4px;
}
.main-table tr:last-child td:last-child {
    border-radius: 0 0 4px 0;
}
.main-table tbody tr td:last-child {
	border-right: 0;
}
.main-table .action-cell .cell-label {
	font-weight: bold;
}
.main-table .action-cell .cell-actions {
	padding-top: 3px;
	font-size: 0.9em;
	display: block;
	visibility: hidden;
}
.main-table .action-cell .cell-actions .edit-action {
	color: #3c7fed;
	border-right: 1px solid #c7c7c7;
	padding-right: 5px;
}
.main-table .action-cell .cell-actions .delete-action {
	color: #bc1313;
	padding-left: 2px;
}
.main-table tbody .active .action-cell .cell-actions {
	visibility: visible;
}
.main-table tbody td.col-center,
.main-table thead th.col-center {
	text-align: center;
}

.add-btn,
.actions-btn,
.cancel-btn {
	background: rgb(13,180,50);
	background: -moz-linear-gradient(top,  rgba(13,180,50,1) 0%, rgba(11,149,41,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(13,180,50,1)), color-stop(100%,rgba(11,149,41,1)));
	background: -webkit-linear-gradient(top,  rgba(13,180,50,1) 0%,rgba(11,149,41,1) 100%);
	background: -o-linear-gradient(top,  rgba(13,180,50,1) 0%,rgba(11,149,41,1) 100%);
	background: -ms-linear-gradient(top,  rgba(13,180,50,1) 0%,rgba(11,149,41,1) 100%);
	background: linear-gradient(to bottom,  rgba(13,180,50,1) 0%,rgba(11,149,41,1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0db432', endColorstr='#0b9529',GradientType=0 );
	border: 1px solid #2dc026;
	padding: 7px 14px;
	border-radius: 4px;
	transition: 0.1s linear;
	cursor: pointer;
	display: inline-block;
	color: #fff;
	font-size: 0.875em;
	box-shadow: inset 0px 1px 0px rgba(255,255,255,0.5), 0 1px 2px #CCCCCC;
	text-shadow: 0 1px 1px rgba(0,0,0,0.2);
	font-weight: bold;
}
.add-btn:hover,
.actions-btn:hover {
	background: -moz-linear-gradient(top,  rgba(11,149,41,1) 0%, rgba(13,180,50,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(11,149,41,1)), color-stop(100%,rgba(13,180,50,1)));
	background: -webkit-linear-gradient(top,  rgba(11,149,41,1) 0%,rgba(13,180,50,1) 100%);
	background: -o-linear-gradient(top,  rgba(11,149,41,1) 0%,rgba(13,180,50,1) 100%);
	background: -ms-linear-gradient(top,  rgba(11,149,41,1) 0%,rgba(13,180,50,1) 100%);
	background: linear-gradient(to bottom,  rgba(11,149,41,1) 0%,rgba(13,180,50,1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0b9529', endColorstr='#0db432',GradientType=0 );
	color: #fff;
}
.add-btn:active,
.actions-btn:active {
	box-shadow: 0 0 4px rgba(0, 0, 0, 0.2) inset, 0 1px 2px #CCCCCC;
}
.add-btn .add-icon {
	background: url(../img/add-icon.png) no-repeat 0 center;
    vertical-align: middle;
    width: 13px;
    height: 13px;
    display: inline-block;
    margin-top: -1px;
    margin-right: 2px;
}
.actions-btn .select-icon {
	background: url(../img/arrow-down.png) no-repeat 0 center;
    vertical-align: middle;
    width: 11px;
    height: 8px;
    display: inline-block;
    margin-top: 0px;
    margin-left: 0px;
}
.actions-btn {
	background: rgb(74,201,248);
	background: -moz-linear-gradient(top,  rgba(74,201,248,1) 0%, rgba(0,157,217,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(74,201,248,1)), color-stop(100%,rgba(0,157,217,1)));
	background: -webkit-linear-gradient(top,  rgba(74,201,248,1) 0%,rgba(0,157,217,1) 100%);
	background: -o-linear-gradient(top,  rgba(74,201,248,1) 0%,rgba(0,157,217,1) 100%);
	background: -ms-linear-gradient(top,  rgba(74,201,248,1) 0%,rgba(0,157,217,1) 100%);
	background: linear-gradient(to bottom,  rgba(74,201,248,1) 0%,rgba(0,157,217,1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#4ac9f8', endColorstr='#009dd9',GradientType=0 );
	border-color: #3ea2c7;
	padding-right: 11px;
}
.actions-btn:hover {
	background: -moz-linear-gradient(top,  rgba(0,157,217,1) 0%, rgba(74,201,248,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,157,217,1)), color-stop(100%,rgba(74,201,248,1)));
	background: -webkit-linear-gradient(top,  rgba(0,157,217,1) 0%,rgba(74,201,248,1) 100%);
	background: -o-linear-gradient(top,  rgba(0,157,217,1) 0%,rgba(74,201,248,1) 100%);
	background: -ms-linear-gradient(top,  rgba(0,157,217,1) 0%,rgba(74,201,248,1) 100%);
	background: linear-gradient(to bottom,  rgba(0,157,217,1) 0%,rgba(74,201,248,1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#009dd9', endColorstr='#4ac9f8',GradientType=0 );
}

.cancel-btn {
	background: #ff5e49; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #ff5e49 0%, #ce5138 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ff5e49), color-stop(100%,#ce5138)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #ff5e49 0%,#ce5138 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #ff5e49 0%,#ce5138 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #ff5e49 0%,#ce5138 100%); /* IE10+ */
	background: linear-gradient(to bottom, #ff5e49 0%,#ce5138 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ff5e49', endColorstr='#ce5138',GradientType=0 ); /* IE6-8 */
	border-color: #c7472d;
	padding-right: 11px;
}

.cancel-btn:hover {
	color: #FFFFFF;
}
	
.controls-wrapper {
	float: left;
}
.controls-wrapper a {
	margin-right: 10px;
	display: block;
    float: left;
}

.table-tools {
	margin-top: 13px;
	float: right;
}
.table-tools-bottom {
	margin-top: 0;
}
.paginator {
	text-align: right;
	font-size: 0.875em;
}
.paginator .prev-btn,
.paginator .next-btn {
}
.paginator .prev-btn a,
.paginator .next-btn a {
	padding: 8px 20px;
}
.paginator .prev-btn {
	float: left;
}
.paginator .prev-btn a {
	background: url(../img/prev-icon.png) no-repeat center center;
	display: block;
}
.paginator .next-btn {
	float: left;
}
.paginator .next-btn a {
	background: url(../img/next-icon.png) no-repeat center center;
	display: block;
}
.paginator .page-numbers {
	float: left;
}
.paginator .page-numbers span {
	
}
.paginator .page-numbers span.current {
	border-radius: 4px;
	border: 1px solid #c7c7c7;
	padding: 3px 15px;
}
.paginator .page-numbers span a {
	padding: 3px 15px;
	color: #51514e;
}
.paginator .page-limit-text {
}
.paginator .page-limit-select {
	margin: 0 3px;
}

.tab-wrapper{
	clear: both;
	width: 100%;
}

ul.tabs{
	display: block;
	width: 100%;
	float: left;
	margin-top: 20px;
	list-style: none;
}
	
ul.tabs li{
	display: inline-block;
}

ul.tabs li .right{
	width: 19px;
	height: 29px;
	display: inline-block;
	line-height: 29px;
	background: url("../img/inactive-tab.png") no-repeat 0px -2px;
	float: right;
}

ul.tabs li.active .right{
	width: 19px;
	height: 29px;
	display: inline-block;
	line-height: 31px;
	background: url("../img/active-tab.png") no-repeat 0px -2px;
	float: right;
	position: relative;
}

ul.tabs li.first{
	margin-left: 22px;
}

ul.tabs li.first a{
	border-left: 1px solid #c7c7c7;
}

ul.tabs li.first .right{
	position: relative;
	z-index: 2;
}

ul.tabs li a{
	float: left;
	display: inline-block;
	height: 27px;
	line-height: 29px;
	padding: 0px 25px;
	padding-left: 35px;
	margin-left: -22px;
	font-family: Helvetica, Arial, sans-serif;
	font-weight: bold;
	font-size: 16px;
	text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.2);
	background: #f9f7f7; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #f9f7f7 85%, #f2f2f2 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(85%,#f9f7f7), color-stop(100%,#f2f2f2)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #f9f7f7 85%,#f2f2f2 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #f9f7f7 85%,#f2f2f2 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #f9f7f7 85%,#f2f2f2 100%); /* IE10+ */
	background: linear-gradient(to bottom, #f9f7f7 85%,#f2f2f2 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9f7f7', endColorstr='#f2f2f2',GradientType=0 ); /* IE6-8 */	
	border-top: 1px solid #c7c7c7;
	border-radius: 5px 0px 0px 0px; 
	-moz-border-radius: 5px 0px 0px 0px; 
	-webkit-border-radius: 5px 0px 0px 0px;
	z-index: 1;
	position: relative;
}

ul.tabs li a:hover{
	color: #51514e;
}

ul.tabs li.active a{
	color: #FFFFFF;
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #4ac9f8 0%, #009dd9 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#4ac9f8), color-stop(100%,#009dd9)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #4ac9f8 0%,#009dd9 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #4ac9f8 0%,#009dd9 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #4ac9f8 0%,#009dd9 100%); /* IE10+ */
	background: linear-gradient(to bottom, #4ac9f8 0%,#009dd9 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#4ac9f8', endColorstr='#009dd9',GradientType=0 ); /* IE6-8 */
	box-shadow: inset 0px 1px 0px 0px rgba(255,255,255,0.5);
	border: #3ea2c7 1px solid;
	border-right: none;
}

.tab-content{
	border-radius: 0px 5px 5px 5px; 
	-moz-border-radius: 0px 5px 5px 5px; 
	-webkit-border-radius: 0px 5px 5px 5px; 
	border: 1px solid #c7c7c7;
	float: left;
	padding: 20px 2%;
	width: 96%;
	margin-top: -2px;
	margin-bottom: 10px;
}

form{
	color: #51514e;
}

form label{
	display: block;
	font-family: Helvetica, Arial, sans-serif;
	font-weight: bold;
	font-size: 16px;
	padding-bottom: 5px;
	margin-top: 20px;
}

form label.checkbox,
form label.radio{
	display: inline;
	font-family: Helvetica, Arial, sans-serif;
	font-size: 14px;
	font-weight: normal;
}

form .description{
	display: block;
	font-size: 12px;
	margin-bottom: 10px;
	font-family: Helvetica, Arial, sans-serif;
	color: #6f6f6f;
}

form input[type=text]{
	border-radius: 5px; 
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px; 
	border: 1px solid #c7c7c7;
	height: 30px;
	font-size: 14px;
	font-family: Helvetica, Arial, sans-serif;
	padding: 0 4px;
	width: 200px;
}

form textarea{
	width: 100%;
	border-radius: 5px; 
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px; 
	border: 1px solid #c7c7c7;
	font-size: 14px;
	font-family: Helvetica, Arial, sans-serif;
	padding: 4px 4px;
}

form select[multiple]{
	border-radius: 5px; 
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px; 
	border: 1px solid #c7c7c7;
	width: 208px;
	font-size: 14px;
}

table.images{
	width: 100%;
	margin: 0 auto;
}

table.images td.first{
	border-bottom: none;
	height: auto;
	padding-left: 10px;
}

table.images td{
	width: 50%;
	height: 150px;
	border-bottom: 40px solid #FFFFFF;
}

table.images td.image{
	width: 140px;
	padding: 10px 10px;
	vertical-align: middle;	
	text-align: left;
}

table.images td.image img{
	border: 10px solid #f2f2f2;
}

table.images td.text{
	/*width: 280px;*/
	padding: 6px 10px;
	padding-right: 40px;
	padding-top: 15px;
	vertical-align: top;
	font-family: Helvetica, Arial, sans-serif;
	font-size: 14px;
	text-align: left;
}

table.images td.text .read-more{
	text-align: right;
	display: block;
	color: #41c1ef;
	font-weight: bold;
}

#accordion{
	width: 100%;
	overflow: hidden;
	list-style: none;
	clear: both;
	margin-bottom: 10px;
}

#accordion li .header{
	cursor: pointer; 
	position: relative; 
	margin-top: 10px; 
	background: #f9f7f7;
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #f9f7f7 85%, #f2f2f2 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(85%,#f9f7f7), color-stop(100%,#f2f2f2));
	background: -webkit-linear-gradient(top, #f9f7f7 85%,#f2f2f2 100%);
	background: -o-linear-gradient(top, #f9f7f7 85%,#f2f2f2 100%);
	background: -ms-linear-gradient(top, #f9f7f7 85%,#f2f2f2 100%);
	background: linear-gradient(to bottom, #f9f7f7 85%,#f2f2f2 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9f7f7', endColorstr='#f2f2f2',GradientType=0 );
	border-radius: 5px; 
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px; 
	border: 1px solid #c7c7c7;
	height: 45px;
	line-height: 45px;
	padding-left: 20px;
	color: #51514e;
	font-size: 14px;
	font-weight: bold;
	font-family: Helvetica, Arial, sans-serif;
	display: block;
}

#accordion li.active .header{
	background: #91c2e0;
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #91c2e0 0%, #91c2e0 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#91c2e0), color-stop(100%,#91c2e0));
	background: -webkit-linear-gradient(top,  #91c2e0 0%,#91c2e0 100%);
	background: -o-linear-gradient(top,  #91c2e0 0%,#91c2e0 100%);
	background: -ms-linear-gradient(top,  #91c2e0 0%,#91c2e0 100%);
	background: linear-gradient(to bottom,  #91c2e0 0%,#91c2e0 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#91c2e0', endColorstr='#91c2e0',GradientType=0 );
	border-radius: 5px 5px 0px 0px; 
	-moz-border-radius: 5px 5px 0px 0px; 
	-webkit-border-radius: 5px 5px 0px 0px;
	height: 50px;
	border: 1px solid #3ea2c7;
	color: #ffffff;
	text-shadow: 0px 1px 2px #000000;	
}

#accordion li .header .actions{
	text-shadow: none;
	color: #51514e; 
	padding-left: 20px;
	font-size: 12px;
	display: none;	
}

#accordion li .header .actions a{
	text-shadow: none; 
	font-weight: normal;
}

#accordion li .header .actions a:hover{
	text-decoration: underline;
}

#accordion li .header .actions a.edit{
	color: #3c7fed;
}

#accordion li.active .header .actions a.edit,
#accordion li.active .header .actions a.delete{
	color: #ffffff;
}

#accordion li .header .actions a.delete{
	color: #bc1313;
}

#accordion li .header .icon{
	float: right;
	margin: 0px 10px;
	text-shadow: none;
	font-weight: bold;
}

#accordion li .header .icon:after{
	content: '─';
}

#accordion li.active .header .icon:after{
	content: '+';
}

#accordion li .content{
	border: 1px solid #c7c7c7;
	margin-top: -2px; 
	position: relative; 
	top: 1px; 
	margin-bottom: 2px; 
	overflow: auto; 
	display: none; 
	zoom: 1; 
	margin-top: -5px;
	border-radius: 5px; 
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px; 
	-webkit-box-shadow: 0px -1px 2px rgba(50, 50, 50, 0.75);
	-moz-box-shadow: 0px -1px 2px rgba(50, 50, 50, 0.75);
	box-shadow: 0px -1px 2px rgba(50, 50, 50, 0.75);
	background: #ffffff;
	padding: 10px 10px;
	overflow-x: hidden;
}

#accordion li .content.table{
	padding: 0 0;
}

#accordion li .content table{
	width: 100%;
	border-bottom: 1px solid #e7e7e7;
	*border-collapse: collapse; /* IE7 and lower */
    border-spacing: 0; 
}

#accordion li .content table th{
	height: 42px;
	line-height: 42px;
	background: #f9f7f7;
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #f9f7f7 85%, #f2f2f2 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(85%,#f9f7f7), color-stop(100%,#f2f2f2));
	background: -webkit-linear-gradient(top, #f9f7f7 85%,#f2f2f2 100%);
	background: -o-linear-gradient(top, #f9f7f7 85%,#f2f2f2 100%);
	background: -ms-linear-gradient(top, #f9f7f7 85%,#f2f2f2 100%);
	background: linear-gradient(to bottom, #f9f7f7 85%,#f2f2f2 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9f7f7', endColorstr='#f2f2f2',GradientType=0 );
	border-bottom: 1px solid #c7c7c7;	
	color: #51514e;
	font-size: 14px;
}

#accordion li .content table th:first-child {
    -moz-border-radius: 5px 0 0 0;
    -webkit-border-radius: 5px 0 0 0;
    border-radius: 5px 0 0 0;
}

#accordion li .content table th:last-child {
    -moz-border-radius: 0 5px 0 0;
    -webkit-border-radius: 0 5px 0 0;
    border-radius: 0 5px 0 0;
}

#accordion li .content table td{
	border-right: 1px solid #E7E7E7;
    color: #6F6F6F;
    font-size: 12px;
    /*height: 46px;*/
    padding: 8px 7px;
    text-align: left;
    vertical-align: top;
}

.center {
	text-align: center !important;
}

#accordion li .content table td img{
	vertical-align: middle;
}

#accordion li .content .rounded{
	border-radius: 5px 5px 0px 0px; 
	-moz-border-radius: 5px 5px 0px 0px; 
	-webkit-border-radius: 5px 5px 0px 0px;
	border: 1px solid #c7c7c7;
	border-bottom: none;
	border-right: none;
	width: 98%;
	margin-left: 2%;
}

#accordion li .content table.sub-table{
	width: 100%;
}


#accordion li .content .action-cell .cell-label {
}
#accordion li .content .action-cell .cell-actions {
	padding-top: 3px;
	font-size: 0.9em;
	display: block;
	visibility: hidden;
}
#accordion li .content .action-cell .cell-actions .edit-action {
	color: #3c7fed;
	border-right: 1px solid #c7c7c7;
	padding-right: 5px;
}
#accordion li .content .action-cell .cell-actions .delete-action {
	color: #bc1313;
	padding-left: 2px;
}
#accordion li .content tbody .active .action-cell .cell-actions {
	visibility: visible;
}

#accordion h4 {
		background: #91c2e0;
		background: url(data:image/svg+xml;base64,);
		background: -moz-linear-gradient(top,  #91c2e0 0%, #91c2e0 100%);
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#91c2e0), color-stop(100%,#91c2e0));
		background: -webkit-linear-gradient(top,  #91c2e0 0%,#91c2e0 100%);
		background: -o-linear-gradient(top,  #91c2e0 0%,#91c2e0 100%);
		background: -ms-linear-gradient(top,  #91c2e0 0%,#91c2e0 100%);
		background: linear-gradient(to bottom,  #91c2e0 0%,#91c2e0 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#91c2e0', endColorstr='#91c2e0',GradientType=0 );
		border-radius: 5px 5px 5px 5px;
		-moz-border-radius: 5px 5px 5px 5px;
		-webkit-border-radius: 5px 5px 5px 5px;
		height: 20px;
		padding: 10px 0 7px 10px;
		border: 1px solid #3ea2c7;
		color: #ffffff;
		text-shadow: 0px 1px 2px #000000;
		margin: 0 0 10px 0;
		font-size: 0.9em
}

.actions-wraper {
	float: left;
}
.actions-wraper .actions-btn {
	width: 112px;
	float: none;
	display: inline-block;
}
.actions-wraper .action-submenu {
	background: none repeat scroll 0 0 #F2F2F2;
    border: 1px solid #C7C7C7;
    border-radius: 4px 4px 4px 4px;
    box-shadow: 0 1px 2px #CCCCCC;
    list-style: none outside none;
    padding: 5px 8px;
    position: absolute;
    z-index:2;
    width: 120px;
    display: none;
}
.actions-wraper .action-submenu li {

}
.actions-wraper .action-submenu li a {
	font-size: 12px;
	line-height: 1.5;
	display: block;
}

form[name=system_group_role_edit] select[multiple=multiple] {
	width: auto;
	height: 370px;
}

/* --- Login Page --- */

#centerbox-page-wrapper {
	position: relative;

	width: auto;
	height: auto;
	margin: 0 auto;
	margin-top: 150px;
	background: url('../img/centerbox-bg.png') center center repeat;

	border-radius: 5px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	-o-border-radius: 5px;

	-webkit-box-shadow: 0px 2px 5px rgba(17, 17, 18, 0.4);
	-moz-box-shadow: 0px 2px 5px rgba(17, 17, 18, 0.4);
	box-shadow: 0px 2px 5px rgba(17, 17, 18, 0.4);

}
#centerbox-page-wrapper.login {
	width: 286px;
	height: 284px;
}
#centerbox-page-wrapper.error {
	width: 286px;
	height: 214px;
}
#centerbox-page-wrapper.error .error-top {
	font-family: "Myriad Pro";
	color: #ffffff;
	font-size: 36px;
	font-weight: bold;
	background: url('../img/warning-icon.png') left center no-repeat;
	height: 50px;
	line-height: 50px;
	padding-left: 50px;
	margin-left: 10px;
	text-shadow: 1px 1px 0px rgba(0, 0, 0, 0.2);
}
#centerbox-page-wrapper.error .error-bottom {
	background: url('../img/error-divider.png') center top repeat-x;
}
#centerbox-page-wrapper.error .error-bottom p {
	font-family: "Myriad Pro";
	font-size: 17px;
	padding-top: 20px;
	text-align: center;
	color: #ffffff;
}
#centerbox-page-wrapper.error .error-bottom a.goback {
	font-family: "Myriad Pro";
	height: 30px;
	line-height: 30px;
	padding: 5px 15px;
	color: #ffffff;
	background-image: linear-gradient(bottom, rgb(206,81,56) 0%, rgb(255,94,73) 100%);
	background-image: -o-linear-gradient(bottom, rgb(206,81,56) 0%, rgb(255,94,73) 100%);
	background-image: -moz-linear-gradient(bottom, rgb(206,81,56) 0%, rgb(255,94,73) 100%);
	background-image: -webkit-linear-gradient(bottom, rgb(206,81,56) 0%, rgb(255,94,73) 100%);
	background-image: -ms-linear-gradient(bottom, rgb(206,81,56) 0%, rgb(255,94,73) 100%);

	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0, rgb(206,81,56)),
		color-stop(1, rgb(255,94,73))
	);

	-webkit-box-shadow: inset 0px 0px 3px rgba(255, 255, 255, 1);
	-moz-box-shadow: inset 0px 0px 3px rgba(255, 255, 255, 1);
	box-shadow: inset 0px 0px 3px rgba(255, 255, 255, 1);

	border: 1px solid #c7472d;
	border-radius: 5px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
#centerbox-page-wrapper.login #centerbox-page-content {
	padding-top: 60px;
}
#centerbox-page-wrapper #centerbox-page-overlay {
	display: none;
	position: absolute;
	width: 100%;
	height: 100%;
	background-image: -webkit-linear-gradient(bottom, rgba(0,0,0,0.1) 0%, rgba(233,233,233,0.05) 100%);
	background-image: linear-gradient(bottom, rgba(0,0,0,0.1) 0%, rgba(233,233,233,0.05) 100%);
	background-image: -o-linear-gradient(bottom, rgba(0,0,0,0.1) 0%, rgba(233,233,233,0.05) 100%);
	background-image: -moz-linear-gradient(bottom, rgba(0,0,0,0.1) 0%, rgba(233,233,233,0.05) 100%);
	background-image: -webkit-linear-gradient(bottom, rgba(0,0,0,0.1) 0%, rgba(233,233,233,0.05) 100%);
	background-image: -ms-linear-gradient(bottom, rgba(0,0,0,0.1) 0%, rgba(233,233,233,0.05) 100%);

	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0, rgba(0,0,0,0.1)),
		color-stop(1, rgba(233,233,233,0.05))
	);

}
#centerbox-page-wrapper /*#centerbox-page-overlay */#centerbox-login-top {
	width: 131px;
	height: 47px;
	position: absolute;
